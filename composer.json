{
    "name": "tructrack",
    "type": "project",
    "require": {
        "php": ">=7",
        "albertcht/invisible-recaptcha": "^1.6",
        "guzzlehttp/guzzle": "^6.2",
        "laravel/framework": "5.4.*",
        "laravel/tinker": "~1.0",
        "myclabs/php-enum": "^1.5"
    },
    "require-dev": {
        "friendsofphp/php-cs-fixer": "^2.3",
        "phpunit/phpunit": "~5.7"
    },
    "autoload": {
        "classmap": [
            "database"
        ],
        "files": [
            "app/Support/helpers.php"
        ],
        "psr-4": {
            "App\\": "app/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Tests\\": "tests/"
        }
    },
    "config": {
        "preferred-install": "dist",
        "sort-packages": true
    }
}
